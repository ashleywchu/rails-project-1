<%= form_tag("/search", method: "get") do %>
	<%="Search Through Database" %>
  <br><%="Name: "%>
	<%= text_field_tag :users_keyword %></br>
  <br><%="Cohort:"%>
  <%= text_field_tag :cohorts_keyword %></br>
  <br><%="Company:"%>
	<%= text_field_tag :companies_keyword %></br>
  <br><%="City:"%>
  <%= text_field_tag :location_keyword %></br>
  <br><%= submit_tag "Search" %></br>
<% end %>