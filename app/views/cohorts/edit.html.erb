<%= form_for(@cohort) do |f| %>
  <%= f.label :cohort %>
  <%= f.text_field :name, :class => "form-control" %><br>

  <%= f.label :start_date %>
  <%= f.date_field :start_date, :class => "form-control" %><br>

  <%= f.label :end_date %>
  <%= f.date_field :end_date, :class => "form-control" %><br>

  <%= f.submit "Submit Cohort", :class => "btn btn-default" %>
<% end %>