<table class = "table" >
	<caption>Search results:</caption>
	<thead>
		<tr>
			<td><%= "#" %></td>
			<td><%= "Result" %></td>
		</tr>
	</thead>
	<tbody>
		<% @fuzzy_search_results.each_with_index do |result, result_index| %>
			<tr>
				<td><%= "#{result_index+1}" %></td>
				<td><%= result.name %></td>
			</tr>
		<% end %>
	</tbody>
</table>
